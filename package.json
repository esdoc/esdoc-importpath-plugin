{
  "name": "esdoc-importpath-plugin",
  "version": "0.0.1",
  "description": "Import Path Plugin For ESDoc",
  "author": "h13i32maru",
  "homepage": "https://github.com/esdoc/esdoc-importpath-plugin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esdoc/esdoc-importpath-plugin"
  },
  "scripts": {
    "build": "rm -rf out && mkdir -p out/src && babel -d out/src src",
    "test": "rm -rf ./test/fixture/esdoc* && mocha -t 10000 -R spec --compilers js:babel-core/register --recursive ./test/src",
    "coverage": "istanbul cover _mocha --report lcovonly -- -t 10000 --compilers js:babel-core/register --recursive ./test/src -R spec",
    "travis": "npm build -s && npm coverage -s && cat ./coverage/lcov.info | coveralls",
    "esdoc": "./node_modules/.bin/esdoc -c esdoc.json"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "coveralls": "~2.11.2",
    "esdoc": "^0.4.8",
    "fs-extra": "^0.22.1",
    "istanbul": "~0.3.16",
    "mocha": "~2.2.5",
    "power-assert": "^0.11.0"
  },
  "keywords": [
    "esdoc"
  ],
  "files": [
    "out/src",
    "README.md"
  ],
  "main": "out/src/Plugin.js"
}
